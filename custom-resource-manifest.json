{
  "kind": "CustomResourceDefinition",
  "spec": {
    "scope": "Namespaced",
    "additionalPrinterColumns": [
      {
        "JSONPath": ".status.lastSync",
        "name": "Last Sync",
        "type": "date"
      },
      {
        "JSONPath": ".status.status",
        "name": "status",
        "type": "string"
      },
      {
        "JSONPath": ".metadata.creationTimestamp",
        "name": "Age",
        "type": "date"
      }
    ],
    "version": "v1",
    "group": "kubernetes-client.io",
    "names": {
      "shortNames": [
        "es"
      ],
      "kind": "ExternalSecret",
      "plural": "externalsecrets",
      "singular": "externalsecret"
    },
    "validation": {
      "openAPIV3Schema": {
        "type": "object",
        "required": [
          "secretDescriptor"
        ],
        "properties": {
          "secretDescriptor": {
            "type": "object",
            "required": [
              "backendType",
              "data"
            ],
            "properties": {
              "backendType": {
                "type": "string",
                "enum": [
                  "secretsManager",
                  "systemManager"
                ]
              },
              "roleArn": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "data": {
                "type": "array",
                "minItems": 1,
                "items": {
                  "type": "object",
                  "required": [
                    "key",
                    "name"
                  ],
                  "properties": {
                    "key": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "property": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "subresources": {
      "status": {}
    }
  },
  "apiVersion": "apiextensions.k8s.io/v1beta1",
  "metadata": {
    "name": "externalsecrets.kubernetes-client.io"
  }
}
